{{!hostmetricsreceiver/cpu}}
-
  metricName: system.cpu.time
  namespace: {{testingId}}
  dimensions:
    -
      name: cpu
      value: {{hostmetricsContext.cpuNames}}
    -
      name: state
      value: [idle, interrupt, nice, softirq, steal, system, user, wait]
    -
      name: host.id
      value: {{hostmetricsContext.hostId}}
